{
  "version": 2,
  "public": true,
  "env": {
    "TIKTOK_CLIENT_ID": "@tiktok_client_id",
    "TIKTOK_CLIENT_SECRET": "@tiktok_client_secret",
    "TIKTOK_REDIRECT_URI": "https://waveupai.fr/api/auth/callback",
    "GEMINI_API_KEY": "@gemini_api_key",
    "FIREBASE_PROJECT_ID": "@firebase_project_id",
    "FIREBASE_CLIENT_EMAIL": "@firebase_client_email",
    "FIREBASE_PRIVATE_KEY": "@firebase_private_key"
  },
  "routes": [
    {
      "src": "^/$",
      "dest": "/api/index"
    },
    {
      "src": "/tiktokg4JvP1aihrNPrCeKlOXg6cCWy18UJGXI.txt",
      "dest": "/api/tiktok-verify"
    },
    {
      "src": "/api/health",
      "dest": "/api/health"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/privacy.html",
      "dest": "/api/privacy"
    },
    {
      "src": "/privacy",
      "dest": "/api/privacy"
    },
    {
      "src": "/terms.html",
      "dest": "/api/terms"
    },
    {
      "src": "/terms",
      "dest": "/api/terms"
    }
  ]
}
