{
  "expo": {
    "name": "WaveUp",
    "slug": "waveup",
    "version": "1.0.0",
    "description": "Analysez vos vidéos TikTok avec l'IA. Obtenez insights, suggestions et stratégie de croissance personnalisée.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "waveup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.waveup.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#00D4FF",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.waveup.tiktok",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00D4FF",
          "dark": {
            "backgroundColor": "#111827"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "c7c9ca1f-88f1-4a6b-b50c-4e48fe9716ee"
      }
    }
  }
}
